{
  "id": "reportportal",
  "version": "1.0.1",
  "name": "ReportPortal",
  "description": "Send spec results to Report Portal",
  "PostInstall": {
    "windows": [],
    "linux": ["chmod", "+x", "ReportPortal.Gauge"],
    "darwin": ["chmod", "+x", "ReportPortal.Gauge"]
